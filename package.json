{
  "name": "next-starter-template",
  "version": "1.0.0",
  "description": "Build a full-stack web application with Next.js.",
  "private": true,
  "cloudflare": {
    "label": "Next.js Framework Starter",
    "products": [
      "Workers"
    ],
    "categories": [],
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/05eee9e0-2c04-48fd-11bf-84e65f839000/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/4e913a56-fd99-4372-0586-9f1364173100/preview",
    "publish": true
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@aws-sdk/s3-request-presigner": "^3.911.0",
    "@opennextjs/cloudflare": "1.3.0",
    "@supabase/supabase-js": "^2.75.1",
    "next": "15.3.3",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "latest",
    "@eslint/eslintrc": "3",
    "@tailwindcss/postcss": "4.1.4",
    "@types/node": "24.0.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vercel/next": "^4.13.4",
    "autoprefixer": "^10.4.21",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "4.1.1",
    "typescript": "5.8.3",
    "wrangler": "^4"
  },
  "scripts": {
    "audit:docs": "bash scripts/md_secret_audit.sh",
    "build": "next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "cf:prep": "npx @cloudflare/next-on-pages@latest --experimental-minify",
    "cf:build": "npm run build && npm run cf:adapt",
    "cf:deploy:staging": "npx wrangler pages deploy .vercel/output/static --project-name lgfc-staging",
    "cf:deploy:prod": "npx wrangler pages deploy .vercel/output/static --project-name lgfc-prod --branch main --commit-dirty=true",
    "check": "npm run build && tsc",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev": "next dev",
    "lint": "next lint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "smoke:preview": "bash scripts/smoke.sh",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "cf:adapt": "npx @cloudflare/next-on-pages@latest",
    "cf:preview": "npx wrangler pages dev .vercel/output/static --port=8788"
  }
}
